{
    "manifest_version": 3,
    "name": "Comments Panel",
    "version": "1.0.0",
    "description": "Post and view comments on any web page using Disqus or Facebook",
    "action": {
      "default_title": "Comments Panel"
    },
    "background": {
      "service_worker": "background.js"
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "permissions": [
      "sidePanel",
      "tabs"
    ]
  }
  